{
  "redirects": [
    {
      "source": "/demo",
      "destination": "/qrcode",
      "statusCode": 301
    },
    {
      "source": "/ref/:code",
      "destination": "https://api.bestdentistduluth.com/ref/:code",
      "statusCode": 302
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
